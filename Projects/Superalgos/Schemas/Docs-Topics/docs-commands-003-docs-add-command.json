        {
            "style": "Title",
            "text": "Parameters"
        },
        {
            "style": "List",
            "text": "Project: It's the project where the page is defined.",
            "updated": 1613764802872
        },
        {
            "style": "List",
            "text": "Docs Page Category: One of the available categories.",
            "updated": 1613764813920
        },
        {
            "style": "List",
            "text": "Docs topic / tutorial / review name: The name of the topic / tutorial / review the new page will belong to.",
            "updated": 1613767524694
        },
        {
            "style": "List",
            "text": "Docs Page Name: The name of the page.",
            "updated": 1613764889322
        },
        {
            "style": "List",
            "text": "Docs Page Numer: Optional. The number affects the position in the corresponding table of contents only. If a Docs Page Number is not provided, the systems assigns one.",
            "updated": 1613764928993
        }
    ]
